{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "Custom starter based on react-boilerplate",
  "repository": {
    "type": "git",
    "url": "git://github.com/rjohnep/react-starter.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "author": "Eugene R.",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "webpack-dev-server --config internals/webpack/webpack.dev.babel.js --color",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "prettify": "prettier --write"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@material-ui/core": "4.1.3",
    "@material-ui/icons": "^4.2.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.1.0",
    "react-router-dom": "5.0.1",
    "redux": "4.0.1",
    "reselect": "4.0.0",
    "sanitize.css": "10.0.0",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.4.4",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@babel/register": "7.4.4",
    "@types/jest": "24.0.15",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.16",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "3.5.0",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "circular-dependency-plugin": "5.0.2",
    "compression-webpack-plugin": "3.0.0",
    "css-loader": "3.0.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-airbnb-typescript": "4.0.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest-cli": "24.8.0",
    "jest-dom": "3.5.0",
    "jest-styled-components": "6.3.3",
    "lint-staged": "9.0.1",
    "pre-commit": "1.2.2",
    "prettier": "1.18.2",
    "react-app-polyfill": "1.0.1",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "8.0.1",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "stylelint": "10.1.0",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.5.2",
    "typescript-plugin-styled-components": "1.4.3",
    "webpack": "4.35.2",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "^3.7.2",
    "whatwg-fetch": "3.0.0"
  }
}
